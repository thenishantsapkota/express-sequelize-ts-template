{
  "name": "express-sequelize-ts-template",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "./dist/index.js",
  "scripts": {
    "start": "npm run migrate && npm run seed && node .",
    "dev": "tsx watch --clear-screen=false --include='./src/data/*' src/index.ts",
    "build": "tsc",
    "prebuild": "rimraf dist",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:write": "prettier . --write"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "i18next": "^24.2.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "socket.io-client": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.8.0",
    "uuid": "^11.1.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "9.0.2",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.0.13",
    "@types/swagger-ui-express": "^4.1.8",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
